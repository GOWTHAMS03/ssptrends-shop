  "clean": "rimraf dist",
  "build": "npm run clean && cross-env NODE_ENV=production webpack --mode production --config webpack/webpack.prod.js",
  "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.dev.js",
  "dev:server": "nodemon",
  "dev": "cross-env npm-run-all --parallel dev:*",
  "start": "cross-env NODE_ENV=production node server/index.js",
  "postinstall": "npm run build",
  "seed:db": "node server/utils/seed.js",
  "test": "echo \"Error: no test specified\" && exit 1",
  "pm2-start": "cross-env NODE_ENV=production pm2 start server/index.js --name my-app",
  "pm2-stop": "pm2 stop my-app",
  "pm2-restart": "pm2 restart my-app",
  "pm2-status": "pm2 status",
  "pm2-logs": "pm2 logs my-app"
}

